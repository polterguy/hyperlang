
/*
 * Creates the event that makes Hyperlang keep alive for the next page load.
 */
create-event:hyperlang.keep-alive.next-pageload

  /*
   * Adding a session variable, that will make sure Hyperlang is automatically
   * loaded during our next pageload.
   */
  p5.web.session.set:hyperlang.keep-alive
    src:bool:true